{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "GCN",
    "dataset": "Fusang_TU_HRRP",

    "out_dir": "out/TUs_graph_classification/",

    "params": {
        "seed": 41,
        "epochs": 600,
        "batch_size": 20,
        "init_lr": 7e-4,
        "lr_reduce_factor": 0.8,
        "lr_schedule_patience": 25,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 138,
        "out_dim": 138,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "self_loop": false
    }
}